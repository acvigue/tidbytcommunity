def main(config):
    station = config.get("station") or ""

    url = "https://mc.chargepoint.com/map-prod/v3/station/info?deviceId={}".format(station)
    res = http.get(url)
    if res.status_code != 200:
        fail("GET %s failed with status %d: %s", url, res.status_code, res.body())
    data = res.json()

    ports = data["portsInfo"]["ports"]
    name = config.get("name") or ' '.join(data["name"])

    takenPorts = 0
    for port in ports:
        if port["status"] != "available":
            takenPorts = takenPorts + 1
    
    portCount = len(ports)

    if data["stationStatus"] != "available":
        image = BOLT
        text = "Station Down  "
        color = "#ff2222"
    elif takenPorts == 0:
        image = BOLT_ANIMATED
        text = "Open: {}{}".format(ports[0]["powerRange"]["max"], ports[0]["powerRange"]["unit"])
        color = "#22ff22"
    elif takenPorts == portCount:
        image = BOLT_GREY
        text = "Occupied"
        color = "#ff2222"
    else:
        image = BOLT_ANIMATED
        text = '{}/{} in use'.format(takenPorts, portCount)
        color = "#ffff22"

    return render.Root(
        delay = 32,
        child=render.Column(children=[
            render.Marquee(
                width=60,
                child=render.Padding(pad=(2,2,0,0), child=render.Row(children=[
                    render.Text("Charge", color="#ff7a14"),
                    render.Text("Point", color="#7a9caf"),
                    render.Text("+", color="#ff7a14"),
                ]))
            ),
            render.Marquee(
                width=60,
                child=render.Padding(pad=(2,2,0,0), child=render.Text(name)),
            ),
            render.Padding(pad=(2,2,2,0), child=
                render.Row(children=[
                    render.Text(text, color=color),
                    render.Image(src=image),
                ], expanded=True, main_align="space_between")
            )
        ])
    )


def get_schema():
    return schema.Schema(
        version="1",
        fields=[
            schema.Text(
                id="station",
                name="Station ID",
                desc="Station IDs",
                icon="lock",
                default="",
            ),
        ],
    )
